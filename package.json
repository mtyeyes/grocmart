{
  "name": "grocmart",
  "version": "1.0.0",
  "description": "Grocery shop template",
  "main": "index.js",
  "scripts": {
    "format": "prettier --config .prettierrc \"**/*.+(ts|tsx|js|jsx|json|css)\" --write",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-update-snapshots": "jest --updateSnapshot",
    "dev": "cross-env NODE_ENV=development webpack",
    "build": "cross-env NODE_ENV=production webpack",
    "start": "cross-env NODE_ENV=development webpack serve --open 'chrome'",
    "predeploy": "cross-env NODE_ENV=production npm run build",
    "deploy": "gh-pages -d dist",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts?(x)": "jest --bail --findRelatedTests"
  },
  "author": "Stan Konovalov",
  "license": "ISC",
  "homepage": "http://mtyeyes.github.io/grocmart/",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-slider": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.13.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.11",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@types/react-animate-on-scroll": "^2.1.2",
    "embla-carousel": "^4.1.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "^17.0.1",
    "react-flip-move": "^3.0.4",
    "react-hook-google-maps": "0.0.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-slider": "^1.1.2",
    "redux-devtools-extension": "^2.13.8"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs",
          "@babel/plugin-transform-react-jsx"
        ]
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scarfSettings": {
    "defaultOptIn": false
  }
}
